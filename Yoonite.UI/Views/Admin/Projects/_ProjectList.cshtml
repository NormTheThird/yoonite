<div id="admin-project-list" class="container-fluid">
    <div class="box">
        <div class="box-inner">
            <div class="table-wrapper">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th class="min-width no-wrap">ID</th>
                            <th>Name</th>
                            <th class="min-width center">Active</th>
                            <th class="min-width no-wrap">Date Created</th>
                        </tr>
                    </thead>
                    <tbody data-bind="foreach: Projects()">
                        <tr>
                            <td class="min-width no-wrap" data-bind="text: Id" />
                            <td data-bind="text: ProjectName" />
                            <td class="min-width center status">
                                <i class="fa fa-check" data-bind="visible: IsActive()"></i>
                                <i class="fa fa-times" data-bind="visible: !IsActive()"></i>
                            </td>
                            <td class="min-width no-wrap" data-bind="text: DateCreated" />
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
